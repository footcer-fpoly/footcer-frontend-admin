{
  "name": "footceradmin",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "version": "react-native-version",
    "postinstall": "jetifier -r",
    "pod": "npx pod-install",
    "rmpod": "cd ios && rm Pods && rm Podfile.lock && pod install && cd ..",
    "start": "react-native start",
    "start2": "react-native start --reset-cache",
    "a": "react-native run-android",
    "i": "react-native run-ios --simulator=\"iPhone 11\"",
    "i8p": "react-native run-ios --simulator=\"iPhone 8 Plus\"",
    "ix": "react-native run-ios --simulator=\"iPhone X\"",
    "t": "react-devtools",
    "r": "npx jetify && cd android && ./gradlew assembleRelease",
    "android": "npx react-native run-android",
    "ios": "react-native run-ios",
    "test": "jest",
    "lint": "eslint .",
    "build:apk": "npx jetify && cd android && ./gradlew assembleRelease",
    "build:aab": "npx jetify && cd android && ./gradlew bundleRelease",
    "clean:android": "cd android && ./gradlew clean && cd ../",
    "clean:ios": "cd ios && xcodebuild clean && cd ../"
  },
  "dependencies": {
    "@react-native-community/async-storage": "^1.12.1",
    "@react-native-community/masked-view": "^0.1.10",
    "@react-native-firebase/app": "^8.4.6",
    "@react-native-firebase/auth": "^9.3.1",
    "@react-navigation/bottom-tabs": "^5.7.3",
    "@react-navigation/native": "^5.7.2",
    "@react-navigation/stack": "^5.8.0",
    "axios": "^0.20.0",
    "react": "16.13.1",
    "react-native": "0.63.3",
    "react-native-elements": "^3.0.0-alpha.1",
    "react-native-flash-message": "^0.1.16",
    "react-native-gesture-handler": "^1.7.0",
    "react-native-hook-image-slider": "^1.2.1",
    "react-native-indicators": "^0.17.0",
    "react-native-maps": "^0.27.1",
    "react-native-modal": "^11.5.6",
    "react-native-reanimated": "^1.10.1",
    "react-native-safe-area-context": "^3.1.1",
    "react-native-screens": "^2.9.0",
    "react-native-table-component": "^1.2.1",
    "react-native-vector-icons": "^7.0.0",
    "react-redux": "^7.2.1",
    "redux": "^4.0.5",
    "redux-devtools-extension": "^2.13.8",
    "redux-persist": "^6.0.0",
    "redux-thunk": "^2.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.11.4",
    "@babel/runtime": "^7.11.2",
    "@react-native-community/eslint-config": "^2.0.0",
    "babel-jest": "^26.3.0",
    "eslint": "^7.7.0",
    "jest": "^26.4.2",
    "metro-react-native-babel-preset": "^0.63.0",
    "react-test-renderer": "16.13.1"
  },
  "jest": {
    "preset": "react-native"
  }
}
